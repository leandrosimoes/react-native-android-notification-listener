{
  "root": true,
  "extends": [
    "defaults",
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "radix": 0,
    "no-prototype-builtins": 0,
    "no-return-await": 0,
    "no-empty": 0,
    "no-param-reassign": 0,
    "react-hooks/exhaustive-deps": 0,
    "react/static-property-placement": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "@typescript-eslint/no-empty-function": 0,
    "import/prefer-default-export": 0,
    "import/extensions": 0,
    "import/named": 0,
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100
      }
    ]
  },
  "plugins": [
    "import",
    "jsx-a11y",
    "prettier",
    "react",
    "react-hooks"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx"],
        "paths": ["src"]
      }
    }
  }
}